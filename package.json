{
    "name": "practise_express",
    "version": "1.0.0",
    "main": "index.js",
    "repository": "git@github.com:meaninglessKyle/practise_express.git",
    "author": "meaninglessKyle <kyle9202@163.com>",
    "license": "none",
    "scripts": {
        "start": "node ./dist/app/app.js ENV=development",
        "build": "rimraf dist && tsc -p tsconfig.json",
        "watch": "nodemon --exec \"npm run build && npm run start\" --watch app --ext ts",
        "prisma:gen": "prisma generate"
    },
    "dependencies": {
        "@prisma/client": "^6.12.0",
        "express": "^5.1.0",
        "module-alias": "^2.2.3"
    },
    "_moduleAliases": {
        "@root": "./dist"
    },
    "devDependencies": {
        "@types/express": "^5.0.3",
        "@types/node": "24",
        "nodemon": "^3.1.10",
        "prisma": "^6.12.0",
        "rimraf": "^6.0.1",
        "typescript": "^5.8.3"
    },
    "prisma": {
        "schema": "db/schema.prisma"
    }
}
